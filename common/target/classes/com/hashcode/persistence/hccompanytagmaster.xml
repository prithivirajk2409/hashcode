<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hashcode.persistence.HCCompanyTagMaster">
	<select id="getCompanyTagsForProblem" parameterType="java.util.Map" resultType="HCCompanyTagMaster">
	<![CDATA[ 
		SELECT
			*
		FROM
			ndcompanytagmaster
		WHERE
			problem_id = #{problemId}
	]]>
	</select>
</mapper>