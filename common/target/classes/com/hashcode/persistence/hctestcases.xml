<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hashcode.persistence.HCTestCases">
	<select id="getTestCasesForProblemId" resultType="HCTestCases">
	<![CDATA[ 
		SELECT 
			*
		FROM
			hctestcases
		WHERE 
			problem_id = #{problemId}
	]]>
	</select>
</mapper>

